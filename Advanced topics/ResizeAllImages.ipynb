    "from PIL import Image\n",
    "import os\n",
    "\n",
    "print(\"Shrink images in the folder\")\n",
    "folder = input(\"./wedding\")\n",
    "for i in os.listdir(folder):\n",
    "    file = f\"{folder}\\\\{i}\"\n",
    "    im = Image.open(file)\n",
    "    im = im.resize((256, 256), Image.ANTIALIAS)\n",
    "    im.save(file)\n",
    "    \n",
    "print(\"images done\")\n",
    "prin(\"test\")"
